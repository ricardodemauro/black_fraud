CREATE TABLE `WebProduct` (
	`Id` BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,
	`Url` VARCHAR(350) NULL DEFAULT '0',
	`VendorCode` VARCHAR(120) NULL DEFAULT '0',
	`Price` DECIMAL(10,2) NULL DEFAULT '0.00',
	`Modified` DATETIME NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
	`Code` VARCHAR(50) NULL DEFAULT '0',
	`DisplayName` VARCHAR(250) NULL DEFAULT '0',
	PRIMARY KEY (`Id`),
	INDEX `Code` (`Code`),
	INDEX `VendorCode` (`VendorCode`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
